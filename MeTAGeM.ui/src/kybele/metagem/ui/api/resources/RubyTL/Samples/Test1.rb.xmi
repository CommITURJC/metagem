<?xml version="1.0" encoding="ISO-8859-1"?>
<rubytl:Transformation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:rubytl="http://org.eclipse.kybele.metagem.rubytl" name="class2table">
  <sourceMetamodels name="ClassM" uri="http://classM.es"/>
  <targetMetamodels name="TableM" uri="http://tableM.es"/>
  <rules xsi:type="rubytl:TopRule" name="r1">
    <from name="classA_name" classname="classA" metamodel="//@sourceMetamodels.0"/>
    <to name="classB_Name" classname="classB" metamodel="//@targetMetamodels.0"/>
    <mapping>
      <bindings property="name">
        <target xsi:type="rubytl:ExpVariable" variable="//@rules.0/@to"/>
        <source xsi:type="rubytl:ExpGet" property="name">
          <source xsi:type="rubytl:ExpVariable" variable="//@rules.0/@from"/>
        </source>
      </bindings>
    </mapping>
    <filter expression="hola"/>
  </rules>
  <rules xsi:type="rubytl:CopyRule" name="copy_RULE">
    <from name="from_c" classname="c_A" metamodel="//@sourceMetamodels.0"/>
    <to name="to_c" classname="c_b" metamodel="//@targetMetamodels.0"/>
    <mapping>
      <bindings property="">
        <target xsi:type="rubytl:ExpVariable" variable="//@rules.1/@to"/>
        <source xsi:type="rubytl:ExpVariable" variable="//@rules.1/@from"/>
      </bindings>
    </mapping>
  </rules>
  <decorators name="decorator_name" body="decorator_body" context="//@rules.0/@from"/>
  <decorators name="d2" body="b2" context="//@rules.0/@from"/>
</rubytl:Transformation>
