context ModelRoot {

	-- El nombre del modelroot es obligatorio y no puede estar vacío
	constraint notEmptyModelRootName {
		check : self.name.isDefined()
		message : 'ModelRoot Failed'
		fix {
     		title : 'ModelRoot'
     		do {
    			self.name := getInputNotEmptyName('ModelRoot'); 
     		}
    	}
    }
}