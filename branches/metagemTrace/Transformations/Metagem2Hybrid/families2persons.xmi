<?xml version="1.0" encoding="ASCII"?>
<metagem:ModelRoot xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:metagem="org.eclipse.kybele.metagem" name="families2persons">
  <sourceModels name="families" path="/Families2Person_Metagem/Families.ecore">
    <elements name="Families" ref="/">
      <elements name="Family" ref="//Family">
        <features name="lastName" ref="//Family/lastName"/>
        <features name="father" ref="//Family/father"/>
        <features name="mother" ref="//Family/mother"/>
        <features name="sons" ref="//Family/sons"/>
        <features name="daughters" ref="//Family/daughters"/>
      </elements>
      <elements name="Member" ref="//Member">
        <features name="firstName" relationElement="//@relations.1/@target/@ManyToOne.0/@source.0 //@relations.2/@target/@ManyToOne.0/@source.0 //@relations.3/@target/@ManyToOne.0/@source.0" ref="//Member/firstName"/>
      </elements>
      <elements name="Father" relationElement="//@relations.0/@source" ref="//Father">
        <features name="familyFather" ref="//Father/familyFather"/>
        <elements name="EGenericType" ref="//Father/@eGenericSuperTypes.0"/>
      </elements>
      <elements name="Mother" relationElement="//@relations.1/@source" ref="//Mother">
        <features name="familyMother" relationElement="//@relations.1/@target/@ManyToOne.0/@source.1" ref="//Mother/familyMother"/>
        <elements name="EGenericType" ref="//Mother/@eGenericSuperTypes.0"/>
      </elements>
      <elements name="Son" relationElement="//@relations.2/@source" ref="//Son">
        <features name="familySon" relationElement="//@relations.2/@target/@ManyToOne.0/@source.1" ref="//Son/familySon"/>
        <elements name="EGenericType" ref="//Son/@eGenericSuperTypes.0"/>
      </elements>
      <elements name="Daughter" relationElement="//@relations.3/@source" ref="//Daughter">
        <features name="familyDaughter" relationElement="//@relations.3/@target/@ManyToOne.0/@source.1" ref="//Daughter/familyDaughter"/>
        <elements name="EGenericType" ref="//Daughter/@eGenericSuperTypes.0"/>
      </elements>
    </elements>
  </sourceModels>
  <targetModels name="persons" path="/Families2Person_Metagem/Persons.ecore">
    <elements name="Persons" ref="/">
      <elements name="Person" ref="//Person">
        <features name="fullName" relationElement="//@relations.0/@target/@zeroToOne.0/@target //@relations.1/@target/@ManyToOne.0/@target //@relations.2/@target/@ManyToOne.0/@target //@relations.3/@target/@ManyToOne.0/@target" ref="//Person/fullName"/>
      </elements>
      <elements name="Male" relationElement="//@relations.0/@target //@relations.2/@target" ref="//Male">
        <elements name="EGenericType" ref="//Male/@eGenericSuperTypes.0"/>
      </elements>
      <elements name="Female" relationElement="//@relations.1/@target //@relations.3/@target" ref="//Female">
        <elements name="EGenericType" ref="//Female/@eGenericSuperTypes.0"/>
      </elements>
    </elements>
  </targetModels>
  <relations xsi:type="metagem:OneToOne" name="father2male">
    <source name="Father" modelComponent="//@sourceModels.0/@elements.0/@elements.2"/>
    <target name="Male" modelComponent="//@targetModels.0/@elements.0/@elements.1">
      <zeroToOne>
        <target name="fullName" modelComponent="//@targetModels.0/@elements.0/@elements.0/@features.0"/>
      </zeroToOne>
    </target>
  </relations>
  <relations xsi:type="metagem:OneToOne" name="mother2female">
    <source name="Mother" modelComponent="//@sourceModels.0/@elements.0/@elements.3"/>
    <target name="Female" modelComponent="//@targetModels.0/@elements.0/@elements.2">
      <ManyToOne>
        <target name="fullName" modelComponent="//@targetModels.0/@elements.0/@elements.0/@features.0"/>
        <source name="firstName" modelComponent="//@sourceModels.0/@elements.0/@elements.1/@features.0"/>
        <source name="familyMother" modelComponent="//@sourceModels.0/@elements.0/@elements.3/@features.0"/>
      </ManyToOne>
    </target>
  </relations>
  <relations xsi:type="metagem:OneToOne" name="son2male">
    <source name="Son" modelComponent="//@sourceModels.0/@elements.0/@elements.4"/>
    <target name="Male" modelComponent="//@targetModels.0/@elements.0/@elements.1">
      <ManyToOne>
        <target name="fullName" modelComponent="//@targetModels.0/@elements.0/@elements.0/@features.0"/>
        <source name="firstName" modelComponent="//@sourceModels.0/@elements.0/@elements.1/@features.0"/>
        <source name="familySon" modelComponent="//@sourceModels.0/@elements.0/@elements.4/@features.0"/>
      </ManyToOne>
    </target>
  </relations>
  <relations xsi:type="metagem:OneToOne" name="daughter2female">
    <source name="Daughter" modelComponent="//@sourceModels.0/@elements.0/@elements.5"/>
    <target name="Female" modelComponent="//@targetModels.0/@elements.0/@elements.2">
      <ManyToOne>
        <target name="fullName" modelComponent="//@targetModels.0/@elements.0/@elements.0/@features.0"/>
        <source name="firstName" modelComponent="//@sourceModels.0/@elements.0/@elements.1/@features.0"/>
        <source name="familyDaughter" modelComponent="//@sourceModels.0/@elements.0/@elements.5/@features.0"/>
      </ManyToOne>
    </target>
  </relations>
</metagem:ModelRoot>
